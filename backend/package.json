{
  "name": "backend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "key:generate": "node scripts/generateRSA256Keys.js",
    "test:server": "NODE_ENV=test jest",
    "babel-node": "babel-node --inspect=0.0.0.0 --presets='@babel/preset-env'",
    "dev": "nodemon --exec npm run babel-node -- src/server.js",
    "build": "babel src --out-dir dist --copy-files",
    "start": "node dist/server.js",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "seed": "node scripts/seed.js",
    "seed:fresh": "npm run migrate:undo:all && npm run migrate && npm run seed"
  },
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "@getbrevo/brevo": "^2.1.1",
    "axios": "^1.7.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "i18n-iso-countries": "^7.11.3",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "mongoose": "^8.5.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "pg": "^8.11.5",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.3",
    "slugify": "^1.6.6",
    "sqlite3": "^5.1.7",
    "stripe": "^15.11.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.6",
    "@babel/core": "^7.24.6",
    "@babel/node": "^7.24.6",
    "@babel/plugin-transform-runtime": "^7.24.6",
    "@babel/preset-env": "^7.24.6",
    "@babel/register": "^7.24.6",
    "@babel/runtime": "^7.24.6",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.3.1",
    "superagent": "^9.0.2",
    "supertest": "^7.0.0"
  }
}
